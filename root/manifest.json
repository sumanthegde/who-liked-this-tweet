{
  "manifest_version": 3,
  "name": "Who Liked This Tweet",
  "version": "1.0",
  "description": "Shows who all liked a tweet in one click",
  "icons": {
    "16": "images/16x16.png",
    "48": "images/48x48.png",
    "128": "images/128x128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*",
        "https://www.twitter.com/*",
        "https://x.com/*",
        "https://www.x.com/*"
      ],
      "js": ["content.js"]
    }
  ]
}
